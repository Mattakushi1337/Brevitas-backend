// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("mongodb://localhost:27017")
}

model users {
    id        String   @id @default(auto()) @map("_id") @db.ObjectId
    username String   
    email     String   @unique
    password String   
}

model visits {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name String 
  surname String 
  discription String  
  mail    String  
  link    String  
  phone   Int 
  userId  String   @db.ObjectId
}